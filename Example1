int box = 39;
int box2;
double co1 = 15.10;
int co2 = 50;
int co2pb = 5;
int boxp1;
double boxp2;
double boxpt;
int box2r;

boxp1 = box * co1
system.println(boxp1);
box2r = box % co2pb;
box2 = box - box2r;
boxp2 = box2 * co2;
system.println(boxp2);
boxp1 = box2r * co1;
boxpt = boxp1 + boxp2;
system.println(boxpt);